    "inversify": "^6.0.2",
    "inversify-binding-decorators": "^4.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.3",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.2.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.1.1",
    "@types/cors": "^2.8.17",
